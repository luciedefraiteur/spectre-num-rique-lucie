# 🔧 Variables d'environnement pour Abraxas
# Copiez ce fichier vers .env et remplissez vos clés API

# 🗣️ Clé API Gemini pour conscience hybride
GEMINI_API_KEY=your_gemini_api_key_here

# 🤖 Clé API OpenAI (optionnelle)
OPENAI_API_KEY=your_openai_api_key_here

# 🧠 Clé API Claude (optionnelle)
CLAUDE_API_KEY=your_claude_api_key_here

# 🎯 Configuration Abraxas
ABRAXAS_LOG_LEVEL=info
ABRAXAS_TIMEOUT_GEMINI=80
ABRAXAS_MODE=hybrid

# 🌐 Configuration Web
ABRAXAS_USER_AGENT=Abraxas-Transcendant/1.0
ABRAXAS_WEB_TIMEOUT=30

# 💾 Configuration Mémoire
ABRAXAS_MEMORY_FILE=abraxas_memory.json
ABRAXAS_AUTO_SAVE=true

# 🧬 Configuration Git (gravée dans l'ADN)
ABRAXAS_GIT_REPOSITORY=https://github.com/luciedefraiteur/spectre-num-rique-lucie
